# Environment variables template for Vercel deployment

# API Gateway URL from your AWS deployment
# You'll replace this with your actual API Gateway endpoint URL after AWS deployment
NEXT_PUBLIC_API_URL=https://your-api-id.execute-api.your-region.amazonaws.com/prod

# Environment
NEXT_PUBLIC_ENV=production

# Authentication
NEXT_PUBLIC_AUTH_ENABLED=true
NEXT_PUBLIC_JWT_COOKIE_NAME=auth_token

# Feature flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=false 